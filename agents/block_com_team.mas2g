use "../bw4t-client-3.8.7-minimal.jar" as environment with 
	clientip ="localhost", clientport="2000", 
	serverip = "localhost", serverport = "8000",
	agentcount = "3", goal = "true", map = "SuperRandom", 
	speed = "100".

