% Mas for testing purposes that uses a seed to make a consistent test environment.
% randomSeed is not yet available in latest master build.
use "../../bw4t-client-3.8.7.jar" as environment with 
	clientip ="localhost", clientport="2000", 
	serverip = "localhost", serverport = "8000",
	agentcount = "2", humancount = "0", launchgui = "false", 
	goal = "true", map = "SuperRandom", speed = "100". %, randomSeed=1.

define robot as agent {
	use "../block_com_team" as main module.
	use "../common/initial" as init module.
	use "../block_com_team_events" as event module.
} 

launchpolicy{
	when * launch robot.
}
