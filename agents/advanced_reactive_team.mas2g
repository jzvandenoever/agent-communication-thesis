use "../bw4t-client-3.8.7-minimal.jar" as environment with 
	clientip ="localhost", clientport="2000", 
	serverip = "localhost", serverport = "8000",
	agentcount = "3", goal = "true", map = "SuperRandom", 
	speed = "100".

define robot as agent {
	use advanced_reactive_team as main module.
	use advanced_reactive_team_init as init module.
	use advanced_reactive_team_events as event module.
}

launchpolicy{
	when * launch robot.
}
